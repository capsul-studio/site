    "stripe": "^8.195.0",
    "tailwindcss": "^3.0.1",
    "timekit-sdk": "^1.19.3",
    "lmdb-store": "^3.0.0-next.91"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
